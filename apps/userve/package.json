    "gatsby-plugin-sharp": "^4.14.1",
    "gatsby-plugin-sitemap": "^5.11.1",
    "gatsby-source-filesystem": "^4.11.1",
    "gatsby-source-storyblok": "^3.1.2",
    "gatsby-storyblok-image": "^0.3.0",
    "gatsby-transformer-json": "^4.12.1",
    "gatsby-transformer-remark": "^5.11.1",
    "gatsby-transformer-sharp": "^4.11.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-head": "^3.4.0",
