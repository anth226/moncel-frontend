    "@percy/cli": "^1.1.4",
    "@types/loadable__component": "^5.13.4",
    "@types/reach__router": "^1.3.10",
    "@types/react-helmet": "^6.1.5",
    "cypress": "^9.6.0",
    "gatsby-cli": "^4.13.1",
    "start-server-and-test": "^1.14.0",
    "storyblok": "^3.10.4",
