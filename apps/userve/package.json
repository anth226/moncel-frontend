    "autoprefixer": "^10.4.4",
    "axios": "^0.27.2",
    "gatsby": "^4.13.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-analytics": "^4.11.0",
    "gatsby-plugin-image": "^2.11.1",
    "gatsby-plugin-loadable-components-ssr": "^4.2.0",
    "gatsby-plugin-manifest": "^4.14.0",
    "gatsby-plugin-mdx": "^3.11.1",
    "gatsby-plugin-netlify-redirect": "^1.0.0",
