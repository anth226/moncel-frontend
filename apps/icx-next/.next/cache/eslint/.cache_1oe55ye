 {\n  const position = useRef(getScrollPosition({ useWindow }))\n\n  let throttleTimeout = null\n\n  const callBack = () => {\n    const currPos = getScrollPosition({ element, useWindow })\n    effect({ prevPos: position.current, currPos })\n    position.current = currPos\n    throttleTimeout = null\n  }\n\n  useLayoutEffect(() => {\n    const handleScroll = () => {\n      if (wait) {\n        if (throttleTimeout === null) {\n          throttleTimeout = setTimeout(callBack, wait)\n        }\n      } else {\n        callBack()\n      }\n    }\n\n    window.addEventListener('scroll', handleScroll)\n\n    return () => window.removeEventListener('scroll', handleScroll)\n  }, deps)\n}","/Users/mkoo/workspace/icc-fix/moncel-frontend/apps/icx-next/components/why-instacert/hero/index.tsx",[],["169","170"],"/Users/mkoo/workspace/icc-fix/moncel-frontend/apps/icx-next/components/why-instacert/recommendation/index.tsx",[],[],"/Users/mkoo/workspace/icc-fix/moncel-frontend/apps/icx-next/lib/cms.ts",[],[],"/Users/mkoo/workspace/icc-fix/moncel-frontend/apps/icx-next/lib/index.ts",[],[],"/Users/mkoo/workspace/icc-fix/moncel-frontend/apps/icx-next/lib/strapi/graphql.ts",[],[],"/Users/mkoo/workspace/icc-fix/moncel-frontend/apps/icx-next/lib/strapi/index.ts",[],[],{"ruleId":"171","severity":1,"message":"172","line":32,"column":29,"nodeType":"173","endLine":32,"endColumn":55},{"ruleId":"174","severity":2,"message":"175","line":7,"column":30,"nodeType":"176","messageId":"177","endLine":7,"endColumn":44,"suppressions":"178"},{"ruleId":"174","severity":2,"message":"175","line":9,"column":30,"nodeType":"176","messageId":"177","endLine":9,"endColumn":44,"suppressions":"179"},"react-hooks/exhaustive-deps","Assignments to the 'throttleTimeout' variable from inside React Hook useLayoutEffect will be lost after each render. To preserve the value over time, store it in a useRef Hook and keep the mutable value in the '.current' property. Otherwise, you can move this variable directly inside useLay